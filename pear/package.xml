            </dir>

            <file name="Observer.php" role="magecommunity" />

          </dir>

          <dir name="sql">

            <dir name="accesscontrol_setup">
              <file name="mysql4-install-1.php" role="magecommunity" />
            </dir>

          </dir>

        </dir>
      </dir>

    </dir>

